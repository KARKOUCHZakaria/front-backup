# Environment Configuration Example
# Copy this file to .env and update with your values

# Database Configuration (Local PostgreSQL)
POSTGRES_DB=creditai
POSTGRES_USER=creditai
POSTGRES_PASSWORD=creditai_password_2024

# Database Configuration (Aiven Cloud - Optional)
AIVEN_DB_URL=jdbc:postgresql://your-aiven-instance.aivencloud.com:20506/defaultdb?sslmode=require
AIVEN_DB_USER=avnadmin
AIVEN_DB_PASS=your_aiven_password_here

# JWT Configuration
JWT_SECRET=YourSuperSecretKeyForJWTTokenGenerationMustBeAtLeast256BitsLong

# ML Service Configuration
ML_SERVICE_URL=http://ml-service:8000

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost

# File Upload
FILE_UPLOAD_DIR=/app/uploads
IDENTITY_SCAN_DIR=/app/uploads/identity-scans

# Logging
LOG_LEVEL=INFO
